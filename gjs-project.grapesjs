ner","classes":["footer-bottom-container"],"components":[{"tagName":"span","type":"text","name":"Footer Copyright","classes":["footer-copyright"],"attributes":{"id":"igs5km"},"components":[{"type":"textnode","content":" 2026 M&Co. Todos los derechos reservados."}]},{"name":"Footer Policies","classes":["footer-policies"],"attributes":{"id":"i83qqq"},"components":[{"type":"link","name":"Footer Policy Link","classes":["gjs-t-link","footer-policy-link"],"attributes":{"href":"#"},"components":[{"type":"textnode","content":"Privacidad"}]},{"type":"link","name":"Footer Policy Link","classes":["gjs-t-link","footer-policy-link"],"attributes":{"href":"#"},"components":[{"type":"textnode","content":"Trminos"}]},{"type":"link","name":"Footer Policy Link","classes":["gjs-t-link","footer-policy-link"],"attributes":{"href":"#","id":"iqfco5"},"components":[{"type":"textnode","content":"Cookies"}]}]}]}]}]},{"type":"script","attributes":{"data-scope":"hamburger-menu","id":"i5cpmb"},"components":[{"type":"textnode","content":"\n  (function(){\n    const btn = document.querySelector('[data-js=\"hamburger-btn\"]');\n    const nav = document.getElementById('ix5rm');\n    if(!btn || !nav) return;\n\n    let prevFocus = null;\n\n    function setAria(isOpen){\n      btn.setAttribute('aria-expanded', String(isOpen));\n      nav.setAttribute('aria-hidden', String(!isOpen));\n      document.body.classList.toggle('menu-open', isOpen);\n    }\n\n    function closeMenu(){\n      nav.classList.remove('is-open');\n      setAria(false);\n      if(prevFocus) prevFocus.focus();\n    }\n\n    function openMenu(){\n      nav.classList.add('is-open');\n      setAria(true);\n      prevFocus = document.activeElement;\n      // Removed auto-focus to avoid \"Inicio\" appearing as active when opening\n    }\n\n    btn.addEventListener('click', function(){\n      const isOpen = nav.classList.contains('is-open');\n      if(isOpen) closeMenu();\n      else openMenu();\n    });\n\n    // Close on outside click\n    document.addEventListener('click', function(e){\n      if(!nav.classList.contains('is-open')) return;\n      const within = nav.contains(e.target) || btn.contains(e.target);\n      if(!within) closeMenu();\n    });\n\n    // Close on escape\n    document.addEventListener('keydown', function(e){\n      if(e.key === 'Escape') closeMenu();\n    });\n\n    // Close when a navigation item is activated (keyboard or click)\n    nav.addEventListener('click', function(e){\n      const link = e.target.closest('a');\n      if(!link) return;\n      closeMenu();\n    });\n\n    // Reset on resize to desktop\n    window.addEventListener('resize', function(){\n      if(window.innerWidth > 992){\n        closeMenu();\n      }\n    });\n\n    // Initialize ARIA on load\n    setAria(false);\n  })();\n"}]},{"type":"script","attributes":{"data<":"","body":""}},{"type":"script","attributes":{"data<":"","body":""}},{"type":"script","attributes":{"data-scope":"active-nav"},"components":[{"type":"textnode","content":"\n  (function(){\n    const nav = document.getElementById('ix5rm');\n    if(!nav) return;\n    const links = Array.from(nav.querySelectorAll('a[href^=\"#\"]'));\n    const map = new Map();\n\n    links.forEach(link=>{\n      const id = link.getAttribute('href').slice(1);\n      const section = document.getElementById(id);\n      if(section){\n        map.set(section, link);\n      }\n    });\n\n    function setActive(link){\n      links.forEach(l=>l.classList.remove('is-active'));\n      if(link) link.classList.add('is-active');\n    }\n\n    // If there is a hash on load, set it\n    if(location.hash){\n      const target = document.getElementById(location.hash.slice(1));\n      if(target && map.has(target)) setActive(map.get(target));\n    }\n\n    // Observe sections to update active state\n    const io = new IntersectionObserver((entries)=>{\n      // Prioritize the entry with highest intersection ratio and in viewport\n      const visible = entries\n        .filter(e => e.isIntersecting)\n        .sort((a,b)=> b.intersectionRatio - a.intersectionRatio);\n\n      if(visible.length){\n        const link = map.get(visible[0].target);\n        if(link) setActive(link);\n      }\n    }, { root: null, rootMargin: '0px 0px -60% 0px', threshold: [0.25, 0.5, 0.75] });\n\n    map.forEach((_, section)=> io.observe(section));\n\n    // Update on click to feel immediate\n    links.forEach(link=>{\n      link.addEventListener('click', ()=>{\n        setActive(link);\n      });\n    });\n  })();\n"}]},{"type":"script","attributes":{"data-scope":"responsive-menu"},"components":[{"type":"textnode","content":"\n  (function(){\n    const body = document.body;\n    const container = document.getElementById('ir8d1'); // header-container\n    const nav = document.getElementById('ix5rm');      // navigation\n    const btn = document.querySelector('[data-js=\"hamburger-btn\"]');\n\n    if(!container || !nav || !btn) return;\n\n    function closeNav(){\n      nav.classList.remove('is-open');\n      btn.setAttribute('aria-expanded','false');\n      nav.setAttribute('aria-hidden','true');\n      body.classList.remove('menu-open');\n    }\n\n    function needsCollapse(){\n      // Always collapse on tablet/mobile\n      if (window.innerWidth <= 992) return true;\n      // Collapse if header content overflows available width\n      return container.scrollWidth > container.clientWidth;\n    }\n\n    function applyCollapse(){\n      const shouldCollapse = needsCollapse();\n      body.classList.toggle('menu-collapsed', shouldCollapse);\n      if(!shouldCollapse){\n        // Ensure menu is closed and ARIA reset when returning to full desktop menu\n        closeNav();\n      }\n    }\n\n    // Initial run and on resize\n    window.addEventListener('resize', applyCollapse);\n    window.addEventListener('load', applyCollapse);\n    // Also trigger once immediately in case script loads late\n    applyCollapse();\n  })();\n"}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","components":[{"tagName":"meta","void":true,"attributes":{"charset":"utf-8"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1"}},{"tagName":"title","type":"text","components":[{"type":"textnode","content":"Consultora de Ciberseguridad | SGSI y Cumplimiento para Pymes"}]},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Poppins:wght@400;600;700&family=Inter:wght@400;500;600&display=swap","rel":"stylesheet"}}]},"docEl":{"tagName":"html","attributes":{"lang":"es"}}},"id":"XPRUMU40BGSfJ7GF","minHeight":"500px"}],"id":"jPS8hkVVapS14EsA"}],"symbols":[{"tagName":"p","type":"text","name":"Hero Subtitle","classes":["hero-subtitle"],"attributes":{"id":"ir92fy"},"__symbols":["io2r4"],"components":[{"type":"textnode","content":"Te acompaamos a construir un Sistema de Gestin de Seguridad de la Informacin robusto, cumplir con normativas aplicables y reducir riesgos tecnolgicos. Profesional, cercano y adaptado a tu realidad.","attributes":{"id":"ivn4f8"},"__symbols":["irooyk"]}]}],"custom":{"projectType":"web","id":""}}